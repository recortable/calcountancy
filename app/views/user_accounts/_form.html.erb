
<% semantic_form_for([:user, @account]) do |form| %>
  <%= form.error_messages %>
  <% form.inputs do %>
    <%= form.input :name %>
    <%= form.input :office_code %>
    <%= form.input :entity_code %>
    <%= form.input :control_code %>
    <%= form.input :number %>
    <%= form.input :address %>
    <%= form.input :owner %>
    <%= form.input :owner_address %>
    <%= form.input :bic %>
    <%= form.input :iban %>
  <% end %>
  <% form.buttons do %>
    <%= form.commit_button %>
  <% end %>
<% end %>